    <div id="content-wrapper" class="d-flex flex-column">
    
        <div id="content">
            <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow"></nav>
            <div class="container-fluid">
                
            <div class="container">
                <div class="card shadow mb-4">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                @if (ViewBag.Travaux != null && ViewBag.Travaux.Count > 0)
                                {
                                    <thead>
                                        <tr>
                                            <th>Id Travaux</th>
                                            <th>Id Categorie</th>
                                            <th>Designation</th>
                                            <th>Unite</th>
                                            <th>PU</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var travail in ViewBag.Travaux)
                                        {
                                            <tr>
                                                <td>@travail.IdTravaux</td>
                                                <td>@travail.IdCategorie</td>
                                                <td>@travail.Designation</td>
                                                <td>@travail.Unite</td>
                                                <td>@travail.PU.ToString("0.00")</td>
                                                <td>
                                                  <a href="/Travaux/Formulaire?idTravaux=@travail.IdTravaux">Update</a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                }
                                else
                                {
                                    <p>Aucun travail trouv√©.</p>
                                }
                            </table>
                            <p id="selectedValue"></p>

                        </div>
                    </div>
                </div>
            </div>
        </div>

