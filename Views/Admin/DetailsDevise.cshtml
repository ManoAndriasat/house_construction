                                                        <th>ID Devise Client</th>
                                                        <th>Catégorie</th>
                                                        <th>Designation</th>
                                                        <th>Unité</th>
                                                        <th>Prix Unité</th>
                                                        <th>Quantité</th>
                                                        <th>Type Maison</th>
                                                        <th>Total</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var detailsDeviseClient in ViewBag.DetailsDeviseClients)
